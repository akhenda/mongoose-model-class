{
  "name": "mongoose-model-class",
  "version": "3.0.0",
  "description": "This module converts a class to a mongoose model.",
  "main": "./dist/src/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "prebuild": "rimraf ./dist",
    "build": "tsc",
    "test": "jest --watch",
    "test:debug": "jest --runInBand --detectOpenHandles",
    "prebuild:types": "rimraf ./build",
    "build:types": "tsc -p ./tsconfig.json --outDir build && api-extractor run",
    "predoc:jsdoc": "rimraf ./docs",
    "docs:jsdoc": "jsdoc -r src/*s -R README.md -d ./docs",
    "predoc:typedoc": "rimraf ./docs",
    "docs:typedoc": "typedoc src --out docs",
    "clean": "rimraf ./build ./dist ./docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cano-koa/mongoose-model-class.git"
  },
  "keywords": [
    "mongodb",
    "mongoose",
    "class",
    "model",
    "schema"
  ],
  "author": "Ernesto Rojas <ernesto20145@gmail.com>",
  "license": "Apache-2.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "joi": "17.9.1",
    "lodash": "4.17.21",
    "mongoose": "6.6.1"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "7.34.4",
    "@types/bcrypt": "5.0.0",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "bcrypt": "5.1.0",
    "eslint": "8.37.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-heimdall": "2.1.1",
    "eslint-plugin-import": "2.27.5",
    "jest": "29.5.0",
    "jest-extended": "3.2.4",
    "jsdoc": "4.0.2",
    "mongodb-memory-server": "8.12.2",
    "prettier": "2.8.7",
    "rimraf": "4.4.1",
    "sha1": "1.1.1",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typedoc": "0.23.28",
    "typescript": "5.0.3"
  }
}
